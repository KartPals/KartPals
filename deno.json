{
    "imports": {
        "neverthrow": "npm:neverthrow@latest",
        "fbtypes": "npm:@firebase/auth-types@latest",
        "chrome": "npm:@types/chrome@latest",
        "vento": "npm:ventojs@latest",
        "deno": "npm:@types/deno@latest",
        "bytebrew": "https://cdn.jsdelivr.net/npm/bytebrew-web-sdk@1.0.1/dist/types/ByteBrew.d.ts",
        "esc": "npm:escape-goat@latest",
        "hminify": "jsr:@minify-html/deno",
        "sbuffer": "https://deno.land/x/smart_buffer/mod.ts",
        "dxhr": "https://deno.land/x/xhr/mod.ts",
        "node": "npm:@types/node@latest",
        "fjparse": "npm:fast-json-parse@latest",
        "wemanifest": "npm:webextension-manifest@latest",
        "esbuild": "https://deno.land/x/esbuild/mod.js",
        "esbuildpd": "jsr:@oazmi/esbuild-plugin-deno",
        "fglob": "npm:fast-glob@latest",
        "esbuildpn": "npm:esbuild-plugins-node-modules-polyfill@latest",
        "obsw.js": "npm:obs-websocket-js@latest",
        "crypto": "node:crypto"
    },
    "compilerOptions": {
        "lib": ["esnext", "dom"],
        "types": ["deno", "chrome", "node"]
    },
    "fmt": {
        "lineWidth": 80,
        "useTabs": false,
        "indentWidth": 4,
        "singleQuote": false,
        "quoteProps": "asNeeded",
        "semiColons": true
    },
    "nodeModulesDir": "auto"
}
